	"tokenColors": [
		{
			"scope": [
				"entity.name.type",
				"support.type",
				"entity.name.function",
				"entity.name.tag",
				"entity.other.attribute-name.class",
				"entity.other.attribute-name.id",
				"support.function"
			],
			"settings": {
				"foreground": "#CAE2DE"
			}
		},
		{
			"scope": [
				"support.other.variable",
				"entity.other.attribute-name",
				"support.class"
			],
			"settings": {
				"foreground": "#AFD4CD"
			}
		},
		{
			"scope": [
				"variable.other.readwrite.alias",
				"meta.property-name",
				"support.type.property-name",
				"variable.language.this",
				"support.variable.property.dom.js",
				"variable"
			],
			"settings": {
				"foreground": "#95C6BD"
			}
		},
		{
			"scope": [
				"support.class.component.js",
				"support.variable.dom",
				"support.variable.object",
				"support.variable.property.process"
			],
			"settings": {
				"foreground": "#698A83"
			}
		},
		{
			"scope": [
				"meta.brace",
				"punctuation",
				"meta.property-value"
			],
			"settings": {
				"foreground": "#3D4E4A"
			}
		},
		{
			"scope": [
				"string",
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.other",
				"variable.other.constant",
				"support.constant",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#C0CA91"
			}
		},
		{
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#8D936E"
			}
		},
		{
			"scope": [
				"comment.block.c"
			],
			"settings": {
				"foreground": "#5A5D4C"
			}
		},
		{
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#3D4E4A",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "meta.parameters comment.block",
			"settings": {
				"foreground": "#5A5D4C",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#C0CA91",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "entity.name.class",
			"settings": {
				"foreground": "#CAE2DE",
				"fontStyle": "underline"
			}
		},
		{
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#CAE2DE",
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#95C6BD",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#F8F8F0"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#F8F8F0"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#83BCD8"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#EFD56C"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#E09190"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#bea5df"
			}
		}
	]
